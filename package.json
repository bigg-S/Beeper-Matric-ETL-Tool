{
  "name": "beeper-matric-etl-tool",
  "type": "module",
  "version": "1.0.0",
  "description": "ETL pipeline for syncing Matrix/Beeper chat data to Supabase with end-to-end encryption support",
  "main": "dist/server/index.js",
  "types": "dist/server/index.d.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:web": "npx next dev",
    "build": "npx next build && tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "npx prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigg-S/Beeper-Matric-ETL-Tool.git"
  },
  "keywords": [
    "Beeper",
    "Matrix",
    "ETL",
    "Supabase",
    "E2E-Encryption"
  ],
  "author": "Stephen",
  "license": "ISC",
  "engines": {
    "node": ">=22.13.0"
  },
  "bugs": {
    "url": "https://github.com/bigg-S/Beeper-Matric-ETL-Tool/issues"
  },
  "homepage": "https://github.com/bigg-S/Beeper-Matric-ETL-Tool#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "fake-indexeddb": "^6.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@matrix-org/matrix-sdk-crypto-nodejs": "^0.3.0-beta.1",
    "@nextui-org/react": "^2.6.11",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "matrix-js-sdk": "^35.1.0",
    "next": "^15.1.4",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.3",
    "uri-js": "^4.2.2",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  }
}
